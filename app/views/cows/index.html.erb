<h1>All the Cows</h1>

<div class="row">
  <div class="col-6">

  <% @cows.each do |cow| %>
    <%= render partial: "shared/cardcow", locals: { cow: cow }  %>
  <% end %>
</div>
       <div class="col-6" class="mapcontainer">
        <div style="width: 100%; height: 600px;"
  data-controller="map"
  data-map-markers-value="<%= @markers.to_json %>"
  data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"></div>
</div>
