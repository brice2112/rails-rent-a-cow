<h1>All the Cows</h1>

<div class="flex-container">
  <% @cows.each do |cow| %>
    <div class="flex-item">
    <div class="card-cows">
        <div class="card-cow-infos">
         <%= cl_image_tag(cow.photo.key, width: 100, height: 100, crop: :fill) %>
          <h2><%= link_to "#{cow.name}", cow_path(cow.id) %></h2>
          <p>Tres belle vache <strong>relevant info</strong> only.</p>
    </div>
  </div>
</div>
  <% end %>
